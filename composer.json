{
    "name": "tuxonice/words",
    "description": "A PHP package for generating pronounceable language-like words",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tlab",
            "email": "example@example.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Tlab\\WordGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tlab\\WordGenerator\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    }
}
